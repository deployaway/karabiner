{
  "title": "@deployaway: Snagit",
  "rules": [
    {
      "description": "Snagit |PrintScreen (F13)| = Capture (F13+Alt+Shift)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "print_screen"
          },
          "to_if_alone": [
            {
              "repeat": true,
              "key_code": "print_screen",
              "modifiers": ["left_alt", "left_shift"]
            }
          ]
        }
      ]
    }
  ]
}
